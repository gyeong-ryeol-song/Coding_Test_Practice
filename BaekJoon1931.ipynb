    "N = int(input())\n",
    "\n",
    "intime = []\n",
    "\n",
    "for i in range(N):\n",
    "    intime.append(list(map(int,input().split())))\n",
    "    \n",
    "intime.sort(key = lambda x: [x[1],x[0]])\n",
    "\n",
    "maxcount = 0\n",
    "start = 0\n",
    "for t in intime:\n",
    "    if t[0]>=start:\n",
    "        start = t[1]\n",
    "        maxcount += 1\n",
    "\n",
    "print(maxcount)\n"
